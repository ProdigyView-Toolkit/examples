    <unset_server>0</unset_server>
    <unset_env>0</unset_env>
    <unset_files>0</unset_files>
    <file_permissions>0644</file_permissions>
    <folder_permissions>0755</folder_permissions>
    <enable_cache>1</enable_cache>
    <file_cache>0</file_cache>
    <cache_time>15</cache_time>
    <force_ssl>0</force_ssl>
    <seo_urls>1</seo_urls>
  </system>
  <sessions>
  	<enable_session_handling>true</enable_session_handling>
  	<session_name>pv_session</session_name>
  	<session_lifetime>2000</session_lifetime>
  	<session_path>/</session_path>
  	<session_domain></session_domain>
  	<session_secure></session_secure>
  	<session_httponly></session_httponly>
    <cookie_lifetime>3000</cookie_lifetime>
    <cookie_path>/</cookie_path>
  	<cookie_domain></cookie_domain>
  	<cookie_secure></cookie_secure>
  	<cookie_httponly></cookie_httponly>
    <track_user>0</track_user>
  </sessions>
  <libraries>
    <ajax_enabled>1</ajax_enabled>
    <jquery_enabled>1</jquery_enabled>
    <mootools_enabled>0</mootools_enabled>
    <prototype_enabled>0</prototype_enabled>
