    <jquery_library>jquery-1.4.2.min.js</jquery_library>
    <mootools_library></mootools_library>
    <prototype_library></prototype_library>
    <admin_ajax_enabled>1</admin_ajax_enabled>
    <admin_jquery_enabled>1</admin_jquery_enabled>
    <admin_mootools_enabled>0</admin_mootools_enabled>
    <admin_prototype_enabled>0</admin_prototype_enabled>
    <admin_ajax_library>ajax_objects.js</admin_ajax_library>
    <admin_jquery_library>jquery-1.4.2.min.js</admin_jquery_library>
    <admin_mootools_library>mootools-1.2.4-core-jm.js</admin_mootools_library>
    <admin_prototype_library>prototype.js</admin_prototype_library>
  </libraries>
