<?xml version="1.0"?>
<site_config>
  <general>
    <site_name>site-Name</site_name>
    <site_address>site_address</site_address>
    <site_index>site_index</site_index>
    <site_slogan>One Framework To Rule Them All</site_slogan>
    <tmp_url>tmp/</tmp_url>
    <admin_url></admin_url>
    <meta_keywords></meta_keywords>
    <meta_description></meta_description>
    <site_online>1</site_online>
    <offline_message></offline_message>
    <show_site_name>1</show_site_name>
    <show_site_slogan>1</show_site_slogan>
    <default_time_zone></default_time_zone>
  </general>
  <email>
    <site_admin_email></site_admin_email>
    <primary_default_email></primary_default_email>
    <mailer>php</mailer>
    <smtp_host></smtp_host>
    <smtp_username></smtp_username>
    <smtp_password></smtp_password>
    <smtp_port></smtp_port>
  </email>
  <system>
    <report_errors>1</report_errors>
    <error_report_level>4</error_report_level>
    <log_errors>1</log_errors>
    <unset_session>0</unset_session>
    <unset_cookie>0</unset_cookie>
    <unset_post>0</unset_post>
    <unset_get>0</unset_get>
